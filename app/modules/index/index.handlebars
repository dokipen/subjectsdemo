<div class="row lead">
  <div class="col-lg-12">
    Some tweeted links about Elysium
  </div>
</div>

<div class="row">
  <div class="col-lg-11 col-lg-offset-1">
    {{#each content}}
      <div class="row">
        <div class="col-lg-12">
          <img {{bindAttr src="embedly.image[0].url"}}>
          <a {{bindAttr href="embedly.url"}}>{{event.data.interaction.content}}</a>
        </div>
      </div>
    {{/each}}
  </div>
</div>
